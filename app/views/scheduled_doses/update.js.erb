
var sdClass = "panel panel-<%= @dose.panel_class %>"

$('#sd-<%= @dose.id %>').attr('class', sdClass);


<% if @dose.side_effect == "" || @dose.side_effect.nil? %>
    $('#sd-<%= @dose.id %>').find('.text-success').html('');
<% else %>
    $('#sd-<%= @dose.id %>').find('.text-success').html('Side Effects Recorded');
<% end %>
